<Window x:Class="Photogrammetry.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525" WindowStartupLocation="CenterScreen" WindowState="Maximized">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="ToolbarRegion" Height="21" VerticalAlignment="Top"/>
        <ContentControl prism:RegionManager.RegionName="ContentRegion" Margin="10,22,10,10"/>
    </Grid>
</Window>
